{
  "name": "styleguide",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "start:dev": "npm run clean && run-p watch:@betatalks/common watch:@betatalks/core watch:styleguide",
    "start:styleguide": "ng serve styleguide",
    "start:e2e:styleguide": "ng serve styleguide --configuration production",
    "watch:@betatalks/common": "ng build @betatalks/common --watch --configuration development",
    "watch:@betatalks/core": "ng build @betatalks/core --watch --configuration development",
    "watch:styleguide": "wait-on dist/betatalks/common/public-api.d.ts dist/betatalks/core/public-api.d.ts && npm run start:styleguide",
    "cypress:open:styleguide": "wait-on http-get://localhost:4200 && ng run styleguide:cypress-open",
    "e2e:styleguide:open": "ng build @betatalks/core && run-p start:styleguide cypress:open:styleguide",
    "e2e:styleguide:run": "ng build @betatalks/core && ng run styleguide:cypress-run",
    "test:@betatalks/common:open": "ng test @betatalks/common",
    "test:@betatalks/common:run": "ng test @betatalks/common --no-watch --no-progress --code-coverage --browsers ChromeHeadless",
    "test:@betatalks/core:open": "ng test @betatalks/core",
    "test:@betatalks/core:run": "ng test @betatalks/core --no-watch --no-progress --code-coverage --browsers ChromeHeadless",
    "test:styleguide:open": "ng test styleguide",
    "test:styleguide:run": "ng test styleguide --no-watch --no-progress --code-coverage --browsers ChromeHeadless",
    "build:styleguide": "ng build styleguide --configuration production",
    "build:@betatalks/common": "ng build @betatalks/common --configuration production",
    "build:@betatalks/core": "ng build @betatalks/core --configuration production && npm run build:@betatalks/core:schematics",
    "build:@betatalks/core:schematics": "cd projects/betatalks/core && npm run build:schematics",
    "publish:@betatalks/common": "cd dist/betatalks/common && npm publish --registry=[path to your registry]",
    "publish:@betatalks/core": "cd dist/betatalks/core && npm publish --registry=[path to your registry]",
    "update:@betatalks/schematics:global": "npm i @betatalks/schematics -g --registry=[path to your registry]",
    "pack:@betatalks/core": "cd dist/betatalks/core && npm pack",
    "pack:@betatalks/common": "cd dist/betatalks/common && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.0",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "cypress": "latest",
    "eslint": "^8.2.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "~4.5.2",
    "wait-on": "^6.0.1"
  }
}
